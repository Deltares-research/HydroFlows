{% import "rule.smk.jinja" as rule_func %}
# This file was generated by hydroflows version {{ version }}

configfile: "{{ configfile }}"

{% for key, values in wildcards.items() %}
{{ key|upper }}={{ values|tojson }}
{% endfor %}

rule all:
    input:
        {% for rule in rules_all %}
        {{ rule }}
        {% endfor %}

{% for rule in rules %}
{% with rule=rule %}
{{ rule_func.add_rule(rule) }}
{% endwith %}
{% endfor %}